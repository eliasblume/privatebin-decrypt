{
  "name": "privatebin-decrypt",
  "type": "module",
  "version": "1.0.2",
  "packageManager": "pnpm@8.10.5",
  "description": "a ts/js lib to decrypt content from privateBin",
  "author": "Elias Blume",
  "license": "MIT",
  "homepage": "https://github.com/eliasblume/privatebin-decrypt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasblume/privatebin-decrypt.git"
  },
  "bugs": "https://github.com/eliasblume/privatebin-decrypt/issues",
  "keywords": [
    "privatebin",
    "decrypt",
    "nodejs"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "update": "pnpx taze major -wi"
  },
  "dependencies": {
    "@scure/base": "^1.1.3",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@antfu/ni": "^0.21.10",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.9.2",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.1.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "unbuild": "^2.0.0",
    "vite": "^5.0.0",
    "vitest": "1.0.0-beta.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
